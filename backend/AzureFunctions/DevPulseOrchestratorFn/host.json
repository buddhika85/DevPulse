{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      },
      "enableLiveMetricsFilters": true
    },
    "logLevel": {
      "Function": "Information",
      "Microsoft.Azure.Functions.Worker": "Information",
      "Microsoft.Azure.Functions.Worker.ServiceBus": "Debug"
    }
  }
}